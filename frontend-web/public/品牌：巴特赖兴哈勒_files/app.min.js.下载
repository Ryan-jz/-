ement(\"canvas\");\n\n      //If we don't support the canvas, we definitely don't support webm alpha video.\n      if (!(canvas.getContext && canvas.getContext('2d'))) {\n        callback(false);\n      }\n\n      // Get the drawing context for canvas.\n      var ctx = canvas.getContext(\"2d\");\n\n      // Draw the current frame of video onto canvas.\n      ctx.drawImage(video, 0, 0);\n      if (ctx.getImageData(0, 0, 1, 1).data[3] === 0) {\n        callback(true);\n      } else {\n        callback(false);\n      }\n    }, false);\n\n    video.addEventListener(\"error\", function() {\n      document.body.removeChild(video);\n      callback(false);\n    });\n\n    video.addEventListener(\"stalled\", function() {\n      document.body.removeChild(video);\n      callback(false);\n    });\n\n    //Just in case\n    video.addEventListener(\"abort\", function() {\n      document.body.removeChild(video);\n      callback(false);\n    });\n\n    var source = document.createElement(\"source\");\n    source.src=\"data:video/webm;base64,\";\n    source.addEventListener(\"error\", function() {\n      document.body.removeChild(video);\n      callback(false);\n    });\n    video.appendChild(source);\n\n    //This is required for IE\n    document.body.appendChild(video);\n  }\n}\n\nvar featureDetector = {\n  isDesktop: function() {\n    return window.matchMedia(\"(min-width: 1280px)\").matches;\n  },\n  isTablet: function() {\n    return window.matchMedia(\"(min-width: 768px)\").matches;\n  },\n  isMobile: function() {\n    return ! (this.isDesktop() || this.isTablet());\n  },\n\tsignalJavaScriptSupport: function() {\n\t\tthis.setFlag('js');\n\t\treturn this;\n\t},\n\tdetectCssSupportOfProperties: function(cssProperties, callback) {\n\t\tthis.detect(css3Detector, cssProperties, callback);\n\t\treturn this;\n\t},\n\tsignalCssSupportOfProperties: function(cssProperties) {\n\t\tthis.detect(css3Detector, cssProperties, function(property) {\n\t\t\tfeatureDetector.setFlag('no-' + propert