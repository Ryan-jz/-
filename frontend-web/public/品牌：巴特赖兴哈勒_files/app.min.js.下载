',\r\n    yesText: '',\r\n    noText: '',\r\n    buttons: [\r\n      dialog.buttons.YES,\r\n      dialog.buttons.NO\r\n    ],\r\n    showCloseButton: false,\r\n    onSubmit: function onDialogSubmit (e) {\r\n      e.preventDefault()\r\n      if (this.options.input) {\r\n        this.value = serialize(this.form, { hash: true })\r\n      }\r\n      return this.close()\r\n    },\r\n    focusFirstInput: true\r\n  }\r\n\r\n  dialog.defaultAlertOptions = {\r\n    buttons: [\r\n      dialog.buttons.YES\r\n    ]\r\n  }\r\n\r\n  dialog.defaultPromptOptions = {\r\n    label: 'Prompt:',\r\n    placeholder: '',\r\n    value: ''\r\n  }\r\n\r\n  dialog.defaultConfirmOptions = {}\r\n\r\n  return dialog\r\n}\r\n\r\nmodule.exports = plugin\r\n\n},{\"deep-clone-simple\":1,\"domify\":2,\"form-serialize\":3}]},{},[4])(4)\n});//# sourceURL=[module]\n//# sourceMappingURL=data:application/json;charset=utf-8;base64,\n//# sourceURL=webpack-internal:///337R\n")},"4GrK":function(module,exports,__webpack_require__){"use strict";eval("\n\n__webpack_require__(\"h54e\");\n__webpack_require__(\"Oyfn\");\nvar featureDetection = __webpack_require__(\"jPZV\");\n\nvar waypoints = [];\n\nvar setCurrentPageByClick = function(e) {\n  var currentPage = e.target;\n  verticalSlider.resetPages();\n  verticalSlider.setCurrentPage(currentPage);\n};\n\nvar verticalSlider = {\n  slider: null,\n  slides: null,\n  pages: null,\n  attach: function() {\n    this.slider = document.querySelector('.vertical-slider');\n    if (! this.slider) {\n      return;\n    }\n    if (! featureDetection.isTablet()) {\n      return;\n    }\n    this.slides = this.slider.querySelectorAll('.slide');\n    this.pages = this.slider.querySelectorAll('.page > a');\n\n    this.togglePagination();\n    this.attachPagination();\n    this.attachSlides();\n  },\n  togglePagination: function() {\n    waypoints.push(new Waypoint.Inview({\n      element: verticalSlider.slider,\n      entered: function(direction) {\n        verticalSlider.slider.cl