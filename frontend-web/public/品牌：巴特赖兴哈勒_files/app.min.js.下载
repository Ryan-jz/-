// Function to modify image sources
function modifyImageSources() {
    // Select all images on the page
    const images = document.querySelectorAll('img');

    // Iterate through each image
    images.forEach(image => {
        let srcValue = image.getAttribute('src');
        
        // Check if the src value starts with 'fileadmin' but not with '/fileadmin'
        if (srcValue.startsWith('fileadmin') && !srcValue.startsWith('/fileadmin')) {
            // Add a slash before 'fileadmin'
            srcValue = '/' + srcValue;
            image.setAttribute('src', srcValue);
        }
    });
}

// Add an event listener for the DOMContentLoaded event
document.addEventListener('DOMContentLoaded', function() {
    // Execute the script when the page is fully loaded
    modifyImageSources();
});


var aTags = document.getElementsByTagName('a');

for (var i = 0; i < aTags.length; i++) {
  var styleAttr = aTags[i].getAttribute('style');

  if (styleAttr && styleAttr.includes('background-image: url(//fileadmin')) {

    var modifiedStyle = styleAttr.replace('background-image: url(//fileadmin', 'background-image: url(/fileadmin');
    aTags[i].setAttribute('style', modifiedStyle);
  }
}



