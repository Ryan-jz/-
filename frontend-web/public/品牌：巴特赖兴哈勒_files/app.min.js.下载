document.addEventListener("DOMContentLoaded", function () {
    const links = document.querySelectorAll('.recipes-spec--filter-by-product');

    links.forEach(function (link) {
      // Durchlaufe alle Textknoten (ignoriere das <img>)
      link.childNodes.forEach(function (node) {
        if (node.nodeType === Node.TEXT_NODE && node.textContent.includes('»Fleur de Sel der Alpen«')) {
          node.textContent = node.textContent.replace(
            '»Fleur de Sel der Alpen«',
            '»Fleur de Sel der Alpen«<br/>'
          );

          // HTML neu setzen, um das <br/> zu rendern
          const span = document.createElement('span');
          span.innerHTML = node.textContent;
          link.replaceChild(span, node);
        }
      });
    });
  });
  
  
  
  document.querySelectorAll('a[aria-state="collapsed"]').forEach(link => {
  link.removeAttribute('aria-state');
  link.setAttribute('aria-expanded', 'false');
});



  document.addEventListener("DOMContentLoaded", () => {
  
  
    // ✅ zentrale Funktion zum (de)aktivieren
    function setContrastMode(active) {
      if (active) {
        document.body.classList.add("contrast-mode");
        sessionStorage.setItem("contrastMode", "true");
        toggleButton.classList.add("active");
      } else {
        document.body.classList.remove("contrast-mode");
        sessionStorage.setItem("contrastMode", "false");
        toggleButton.classList.remove("active");
      }
    }
  
  
  
const oldButton = document.querySelector('a[href="#contrastmode"]');
if (oldButton) {
  const newButton = oldButton.cloneNode(true); // Klonen (ohne EventListener)
  newButton.removeAttribute("target");
  newButton.innerHTML = '<i class="fa fa-eye"></i>';
    	newButton.classList.remove("external");
    	newButton.removeAttribute("target");
    	newButton.classList.add("loaded");

  newButton.addEventListener("click", (event) => {
    event.preventDefault();
    if (sessionStorage.getItem("contrastMode") === "true") {
      setContrastMode(false);
    } else {
      setContrastMode(true);
    }
  });

  oldButton.parentNode.replaceChild(newButton, oldButton);
}
  
  
  
  
    const toggleButton = document.querySelector('a[href="#contrastmode"]');



    // ✅ Kontrastmodus aus Session wiederherstellen
    const savedContrast = sessionStorage.getItem("contrastMode");
    if (savedContrast === "true") {
      setContrastMode(true);
    }

    // ✅ Kontrastmodus via URL-Parameter aktivieren/deaktivieren
    const urlParams = new URLSearchParams(window.location.search);
    const contrastParam = urlParams.get("testcontrastmode");

    if (contrastParam === "on") {
      setContrastMode(true);
    } else if (contrastParam === "off") {
      setContrastMode(false);
    }
  });



