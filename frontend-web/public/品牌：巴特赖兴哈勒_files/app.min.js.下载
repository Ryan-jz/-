    li.innerHTML = '<a href="https://www.bad-reichenhaller.de/eine-prise-selbstgemacht/" title="Zum Genuss-Coach" target="_blank"><span>Genuss-Coach</span></a>';
    nav.insertBefore(li, nav.firstChild);
  }
});